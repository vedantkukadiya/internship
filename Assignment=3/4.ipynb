    "class Solution:\n",
    "    def Number(self, nums):\n",
    "        count =0\n",
    "        while nums > 0:\n",
    "            if nums %2==0:\n",
    "                nums = nums//2\n",
    "            else:\n",
    "                nums = nums -1\n",
    "            count +=1\n",
    "        return count\n",
    "s=Solution()\n",
    "print(s.Number(30))"
   ]
  }
