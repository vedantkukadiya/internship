    "class Solution:\n",
    "    def Bits(self, nums: int) -> list[int]:\n",
    "      ans = [0] * (nums+1)\n",
    "      for i in range(nums+1):\n",
    "        ans[i] = ans[i // 2] + (i % 2)\n",
    "        \n",
    "      return ans\n",
    "s = Solution()\n",
    "print(s.Bits(8))"
   ]
  }
