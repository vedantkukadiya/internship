    "class Solution:\n",
    "    def maxValue(self, nums: list[int]) -> int:\n",
    "\n",
    "        max_val=max(nums)\n",
    "        nums.remove(max_val)\n",
    "\n",
    "        max1_val=max(nums)\n",
    "        return (max_val-1)*(max1_val-1)\n",
    "s=Solution()\n",
    "print(s.maxValue([5,20,2,6]))        "
   ]
  }
