    "class Solution:\n",
    "    def xorOperation(self, n: int, start: int) -> int:\n",
    "        result = 0\n",
    "        for i in range(n):\n",
    "            result ^= start + 2 * i\n",
    "        return result\n",
    "s=Solution()\n",
    "print(s.xorOperation(7,8))  "
   ]
  }
